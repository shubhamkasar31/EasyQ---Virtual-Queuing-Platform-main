    <style>
        #msg{
            color: rgb(112, 58, 58);
            font-family: 'Roboto Mono', monospace;
            display: flex;
            justify-content: center;
            font-weight: bold;
            background-color: rgb(104, 240, 149);
            border-radius: 5px;
        }
    </style>
    
</head>

<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">
                EasyQ
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="#">
                            Register
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/adminqueue">
                            Queue
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/adminqueue">
                            Contact
                        </a>
                    </li>
                </ul>
                <span class="navbar-text">
                    <a class="nav-link " href="#">
                        <%=email%>
                    </a>
                </span>
                <span class="navbar-text">
                    <a class="nav-link " href="/logoutAdmin">
                        Logout
                    </a>
                </span>
            </div>
        </div>
    </nav>

    <div class='container my-5' style=
        "border: 1px solid #ced4da;
        border-radius: 0.25rem;
        padding : 10px 30px 10px 30px">
        <h2 style="text-Align: center">Register Your Shop</h2>
        <form action="/homeAdmin" method="post">
            <p id="msg"> <%=(typeof alertMsg!='undefined')? alertMsg:''%></p>
            <div class="mb-3">
                <label for="Name">Owner Name</label>
                <input type="text" class="form-control" id="Name" name="ownername" required/>
            </div>
            <div class="mb-3">
                <label for="Name">Shop Name</label>
                <input type="text" class="form-control" id="Name" name="shopname" required/>
            </div>
            <div class="mb-3">
                <label for="Domain">Domain</label>
                <input type="text" class="form-control" id="Domain" name="domain" required/>
            </div>
            <div class="mb-3">
                <label for="Address Line 1">Address Line 1</label>
                <input type="text" class="form-control" id="Address Line 1" name="add1" required/>
            </div>
            <div class="mb-3">
                <label for="Address Line 2">Address Line 2</label>
                <input type="text" class="form-control" id="Address Line 2" name="add2" required/>
            </div>
            <div class="mb-3">
                <label for="Moblie No.">Moblie No.</label>
                <input type="text" class="form-control" id="Moblie No." name="mobno" required/>
            </div>
            <!-- <div class="mb-3">
                <label for="exampleInputcertificate" class="form-label"> Upload Certificate </label><br>
                <input type="file" id="image_uploads" name="image_uploads" accept=".jpg, .jpeg, .png, .pdf" multiple>
            </div> -->
            
            <div class="mb-3">
                <label class="form-label" for="appt">Select Ideal a time :  From </label>
                <input style=
                "width: 20%;
                padding: 0.375rem 0.75rem;
                font-size: 1rem;
                font-weight: 400;
                line-height: 1.5;
                color: #212529;
                background-color: #fff;
                background-clip: padding-box;
                border: 1px solid #ced4da;
                border-radius: 0.25rem" type="time" id="apptf" name="starttime" required/>
                <label class="form-label mx-3" for="appt">To : </label>
                <input style=
                    "width: 20%;
                    padding: 0.375rem 0.75rem;
                    font-size: 1rem;
                    font-weight: 400;
                    line-height: 1.5;
                    color: #212529;
                    background-color: #fff;
                    background-clip: padding-box;
                    border: 1px solid #ced4da;
                    border-radius: 0.25rem"
                 type="time" id="apptt" name="endtime" required/>
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>

    </div>
    








    <!-- MDB -->
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js"
        integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p"
